#include <stdio.h>
#include <string.h>

typedef struct Message{
    int c[128];
}Message;

int getMsgID(Message *packet){
    return packet->c[0];
}

int getSourceID(Message *packet){
    return packet->c[1];
}

int getDestinationID(Message *packet){
    return packet->c[2];
}

int getTrailingMsg(Message *packet){
    return packet->c[3];
}

int getFunctionID(Message *packet){
    return packet->c[4];
}

//length of function is 123
void getPayload(Message *packet, char* answer){
    int i=0;
    while (i<123){
        answer[i]=packet->c[i+5];
        i++;
    }
}


void setMSGID(Message *packet, int msgId){
    packet->c[0] = msgId;
}

void setSourceID(Message *packet, int sourceId){
    packet->c[1] = sourceId;
}

void setDestinationID(Message *packet, int destId){
    packet->c[2] = destId;
}

void setTrailingMsg(Message *packet, int trailMsg){
    packet->c[3] = trailMsg;
}

void setFunctionID(Message *packet, int funcId){
    packet->c[4] = funcId;
}

void setPayload(Message *packet, char* payload){
    int i=0;
    while (i<128){
        packet->c[i+5] = payload[i];
        i++;
    }
}


int main()
{
    Message m;
    char answer[123];
    setFunctionID(&m, 5);
    setPayload(&m, "0123");
    int a = (int)getFunctionID(&m);
    printf("function id: %d", &a);
    getPayload(&m, &answer);
    printf("%s", answer);
}
